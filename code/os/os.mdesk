@table_gen_enum OS_Key:
{
    @expand(OS_KeyTable a)
        `OS_Key_$(a.name),`;
    `OS_Key_COUNT`;
}

@table_gen_data(String8)
os_g_key_string_table:
{
    @expand(OS_KeyTable a)
        `Str8LitComp("$(a.string)"),`;
}
